    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/uuid": "^8.3.3",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "del-cli": "^4.0.1",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",

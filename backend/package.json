{
  "name": "sadalsuud-backend",
  "version": "1.0.0",
  "description": "check if one wins the price",
  "author": "Yue Chang",
  "scripts": {
    "lint:ts": "tslint --project .",
    "fix:ts": "npm run lint:ts -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./{src,test}/**/*.ts\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean:build": "del-cli build -f",
    "clean:dist": "del-cli dist -f",
    "clean:tmp": "del-cli tmp -f",
    "clean:coverage": "del-cli coverage -f",
    "clean": "run-p clean:*",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "build": "webpack --config webpack.config.js",
    "pre:commit": "run-s lint test",
    "pre:deploy": "run-s clean build"
  },
  "license": "ISC",
  "dependencies": {
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "del-cli": "^4.0.1",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "zip-webpack-plugin": "^4.0.1"
  }
}
